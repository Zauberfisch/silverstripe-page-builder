{
	"name": "zauberfisch/silverstripe-page-builder",
	"description": "SilverStripe module to allow for modular, block based content in SilverStripe",
	"type": "silverstripe-vendormodule",
	"keywords": [
		"silverstripe",
		"content",
		"elements",
		"block",
		"blocks",
		"contentblock",
		"pagebuilder",
		"page-builder",
		"page-blocks",
		"content-blocks"
	],
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Zauberfisch",
			"email": "code@zauberfisch.at"
		}
	],
	"require": {
		"silverstripe/framework": "^4.0",
		"silverstripe/admin": "^1.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^5.7",
		"squizlabs/php_codesniffer": "^3.0"
	},
	"autoload": {
		"psr-4": {
			"zauberfisch\\PageBuilder\\": "src/",
			"zauberfisch\\PageBuilder\\Tests\\": "tests/"
		}
	},
	"extra": {
		"expose": [
			"client/dist",
			"client/lang"
		]
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
